<main>
  <p id="notice"></p>
  <div class="container">
    <div class="row">
      <div class="mx-auto">

      <h2><%= resource_name.to_s.humanize %> info</h2>

      <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
        <%= render "devise/shared/error_messages", resource: resource %>






        <div class="actions">
        <%= f.submit "Update" %>
        </div>
      </div>
    </div>
  </div>
</main>

<% end %>



